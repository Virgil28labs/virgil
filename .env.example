# Frontend Environment Variables (VITE_ prefix for client-side access)
# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# LLM Service Configuration
VITE_LLM_API_URL=http://localhost:5002/api/v1

# Google Maps API Configuration (if needed)
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# OpenWeatherMap API Configuration (if needed)
VITE_OPENWEATHER_API_KEY=your_openweather_api_key

# Backend Environment Variables (NO VITE_ prefix - server-side only)
# OpenAI API Configuration (SECURE - Backend only)
OPENAI_API_KEY=your_openai_api_key

# LLM Server Configuration
LLM_SERVER_PORT=5002
NODE_ENV=development

# Production Security
ALLOWED_ORIGINS=https://yourdomain.com